<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.throne212.fupin.domain">
	<class name="Diqu" table="fp_diqu">
		<id column="id" name="id" type="long">
			<generator class="increment" />
		</id>
		<discriminator column="diqu_type" type="string" force="false" insert="true" />
		<property name="name" length="100"></property>
		<property name="remark" length="2048"></property>
		<!-- 公用字段 -->
		<property name="createDate" column="create_date"></property>
		<property name="createName" column="create_name"></property>
		<property name="segment1"></property>
		<property name="segment2"></property>
		<property name="segment3"></property>
		<property name="segment4"></property>
		<property name="segment5"></property>
		<!-- 公用字段 end -->
		<subclass discriminator-value="shi" extends="Diqu" name="Shi">
		</subclass>
		<subclass discriminator-value="area" extends="Diqu" name="Area">
			<many-to-one name="shi" column="shi_id" lazy="false"></many-to-one>
		</subclass>
		<subclass discriminator-value="zhen" extends="Diqu" name="Zhen">
			<many-to-one name="area" column="area_id" lazy="false"></many-to-one>
			<many-to-one name="shiWorkOrg" column="shiWorkOrg_id" lazy="false"></many-to-one>
			<many-to-one name="areaWorkOrg" column="areaWorkOrg_id" lazy="false"></many-to-one>
		</subclass>
		<subclass discriminator-value="cun" extends="Diqu" name="Cun">
			<many-to-one name="zhen" column="zhen_id" lazy="false"></many-to-one>
			<many-to-one name="org"  lazy="false"></many-to-one>
			<many-to-one name="team" column="team_id"  lazy="false"></many-to-one>
			<property name="dian"></property>
			<property name="dibaoFamily"></property>
			<property name="dibaoPerson"></property>
			<property name="familyNum"></property>
			<property name="house"></property>
			<property name="income"></property>
			<property name="labor"></property>
			<property name="mianji"></property>
			<property name="needBanFamily"></property>
			<property name="needBanPerson"></property>
			<property name="office"></property>
			<property name="outLabor"></property>
			<property name="personNum"></property>
			<property name="poorFamilyNum"></property>
			<property name="poorPersonNum"></property>
			<property name="trainingNum"></property>
			<property name="transNum"></property>
			<property name="tv"></property>
			<property name="water"></property>
			<property name="weiHouse"></property>
			<property name="ying"></property>
			<property name="you"></property>
			<property name="item" length="2048"></property>
			<property name="pinyin"></property>
			<!-- 新增字段 -->
			<property name="poorFamilyNum1"></property>
			<property name="poorPersonNum1"></property>
			<property name="poorFamilyNum2"></property>
			<property name="poorPersonNum2"></property>
			<property name="poorFamilyNum3"></property>
			<property name="poorPersonNum3"></property>
			<property name="poorFamilyNum4"></property>
			<property name="poorPersonNum4"></property>
			
			<property name="tel"></property>
			<property name="pureWater" column="pure_water"></property>
			<property name="light"></property>
		</subclass>
	</class>
</hibernate-mapping>
