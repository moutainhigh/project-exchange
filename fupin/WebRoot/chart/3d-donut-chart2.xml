<?xml version="1.0" encoding="UTF-8"?>
<settings> 
  <font>Tahoma</font>        
  <pie>
    <inner_radius>40</inner_radius>    
    <height>20</height>                
    <angle>10</angle>
    <gradient>radial</gradient>                  
    <gradient_ratio>-50,0,0,-50</gradient_ratio>    
    <colors>#CAE1F3,#ACC65D,#F3D36E,#F3B38A</colors>        
  </pie>
  
  <animation>
    <start_time>2</start_time>         
    <start_effect>bounce</start_effect>
    <pull_out_time>1.5</pull_out_time> 
    <pull_out_effect>Bounce</pull_out_effect>
    <pull_out_only_one>true</pull_out_only_one>       
  </animation>
  
  <data_labels>
    <show>
       <![CDATA[{title}: {value} 户]]>        
    </show>
    <line_color>#000000</line_color>           
    <line_alpha>15</line_alpha>                
    <hide_labels_percent>3</hide_labels_percent>    
  </data_labels>
  
  <background>
  	<color>#D6EBFF</color>
  </background>
  
  <balloon>                                     
    <show>
       <![CDATA[{title}: {value} 户)]]>  
    </show>
  </balloon>
  
  <legend>                     
    <enabled>false</enabled>   
  </legend>    
 
  <labels>                     
    <label>
      <x>0</x>                 
      <y>40</y>                
      <align>center</align>      
      <text_size>14</text_size> 
      <text>                    
        <![CDATA[<b>规划到户情况汇总</b>]]>
      </text>        
    </label>
  </labels>
</settings>