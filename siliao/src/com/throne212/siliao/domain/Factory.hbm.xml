<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.throne212.siliao.domain">
	<class name="Factory" table="YH_FACTORY">
		<id column="id" name="id" type="long">
			<generator class="increment" />
		</id>
		<property name="creator"></property>
		<property name="date" column="create_date"></property>
		<property name="head"></property>
		<property name="name" not-null="true"></property>
		<property name="remark"></property>
	</class>
</hibernate-mapping>
