<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
  <data-sources />
  <form-beans />
  <global-exceptions />
  <global-forwards />
  <action-mappings >
    <action path="/test" type="com.throne212.oa.action.TestAction">
    	<forward name="test" path="/test.jsp"></forward>
    	<forward name="report_test" path="/report/test.jsp"></forward>
    </action>
    <!-- 乡村医生 -->
    <action path="/doctor" type="com.throne212.oa.action.DoctorAction" parameter="method">
    	<forward name="edit" path="/doctor/edit.jsp"></forward>
    	<forward name="list" path="/doctor/list.jsp"></forward>
    	<forward name="dic_list" path="/doctor/dic_list.jsp"></forward>
    	<forward name="dic_edit" path="/doctor/dic_edit.jsp"></forward>
    	<forward name="print" path="/doctor/print.jsp"></forward>
    	<forward name="print2" path="/doctor/print2.jsp"></forward>
    	<forward name="print3" path="/doctor/print3.jsp"></forward>
    	<forward name="print4" path="/doctor/print4.jsp"></forward>
    </action>
    <!-- 人事档案 -->
    <action path="/person" type="com.throne212.oa.action.PersonAction" parameter="method">
    	<forward name="edit" path="/person/edit.jsp"></forward>
    	<forward name="list" path="/person/list.jsp"></forward>
    	<forward name="dic_list" path="/person/dic_list.jsp"></forward>
    	<forward name="dic_edit" path="/person/dic_edit.jsp"></forward>
    </action>
    <!-- 人事文件 -->
    <action path="/file" type="com.throne212.oa.action.PersonFileAction" parameter="method">
    	<forward name="edit" path="/personfile/edit.jsp"></forward>
    	<forward name="list" path="/personfile/list.jsp"></forward>
    	<forward name="dic_list" path="/personfile/dic_list.jsp"></forward>
    	<forward name="dic_edit" path="/personfile/dic_edit.jsp"></forward>
    </action>
    <!-- 报表 -->
    <action path="/report" type="com.throne212.oa.action.ReportAction" parameter="method">
    	<forward name="dic_list" path="/report/dic_list.jsp"></forward>
    	<forward name="dic_edit" path="/report/dic_edit.jsp"></forward>
    	<forward name="main" path="/report/main.jsp"></forward>
    	<forward name="default" path="/report/default.jsp"></forward>
    	<forward name="year" path="/report/year.jsp"></forward>
    	<forward name="month" path="/report/month.jsp"></forward>
    	<forward name="season" path="/report/season.jsp"></forward>
    	<forward name="org" path="/report/org.jsp"></forward>
    	<forward name="compare" path="/report/compare.jsp"></forward>
    	<forward name="summary" path="/report/summary.jsp"></forward>
    	<forward name="test" path="/report/test.jsp"></forward>
    </action>
	<!-- 母婴（人员） -->
    <action path="/worker" type="com.throne212.oa.action.WorkerAction" parameter="method">
    	<forward name="edit" path="/worker/edit.jsp"></forward>
    	<forward name="list" path="/worker/list.jsp"></forward>
    	<forward name="dic_list" path="/worker/dic_list.jsp"></forward>
    	<forward name="dic_edit" path="/worker/dic_edit.jsp"></forward>
    </action>
    <!-- 母婴（机构） -->
    <action path="/org" type="com.throne212.oa.action.OrgAction" parameter="method">
    	<forward name="edit" path="/org/edit.jsp"></forward>
    	<forward name="list" path="/org/list.jsp"></forward>
    	<forward name="dic_list" path="/org/dic_list.jsp"></forward>
    	<forward name="dic_edit" path="/org/dic_edit.jsp"></forward>
    	<forward name="print" path="/org/print2.jsp"></forward>
    </action>
    <!-- 出生证 -->
    <action path="/birth" type="com.throne212.oa.action.BirthAction" parameter="method">
    	<forward name="edit" path="/birth/edit.jsp"></forward>
    	<forward name="list" path="/birth/list.jsp"></forward>
    	<forward name="dic_list" path="/birth/dic_list.jsp"></forward>
    	<forward name="dic_edit" path="/birth/dic_edit.jsp"></forward>
    </action>
  </action-mappings>

  <message-resources parameter="com.throne212.oa.ApplicationResources" />
</struts-config>

